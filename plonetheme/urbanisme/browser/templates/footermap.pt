<div tal:condition="not:exists:python:here.fr.lepermisdurbanisme">
  <p>&nbsp;</p>
</div>
<div tal:condition="exists:python:here.fr.lepermisdurbanisme">
<div tal:define="object python:here.fr.lepermisdurbanisme">
	<metal:block tal:define="obj_title python:object.title_or_id();
							 mappen python:object.getFolderContents(contentFilter = {'portal_type' : ['Folder']});">
		<tal:block> 
			<div>
				<p tal:content="structure obj_title">titel</p>
				<ul>
					<tal:block repeat="map mappen">                
						<li class="bodytt" tal:condition="python: not map.getObject().exclude_from_nav()"><a href="" tal:attributes="href map/getURL;"><tal:block tal:content="map/Title" /></a>
							<tal:block tal:define="subfolders python:map.getObject().getFolderContents(contentFilter = {'portal_type' : ['Folder', 'Document']});">
								<ul tal:condition="subfolders">
									<tal:block repeat="subfolder subfolders">
										<li tal:condition="python: not subfolder.getObject().exclude_from_nav()">
											<a href="" tal:attributes="href subfolder/getURL;"><tal:block tal:content="subfolder/Title" /></a>
										</li>
									</tal:block>
								</ul>
							</tal:block>
						</li>
					</tal:block>
				</ul>
			</div>
		</tal:block>
	</metal:block>
</div>
</div>
</div>

