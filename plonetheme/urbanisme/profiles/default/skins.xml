<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False" default_skin="Urbanisme Theme">

 <object name="urbanisme_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.urbanisme:skins/urbanisme_images"/>
 <object name="urbanisme_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.urbanisme:skins/urbanisme_templates"/>
 <object name="urbanisme_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.urbanisme:skins/urbanisme_styles"/>

 <skin-path name="Urbanisme Theme" based-on="Plone Default">
  <layer name="urbanisme_images"
     insert-after="custom"/>
  <layer name="urbanisme_templates"
     insert-after="urbanisme_images"/>
  <layer name="urbanisme_styles"
     insert-after="urbanisme_templates"/>
 </skin-path>

</object>
